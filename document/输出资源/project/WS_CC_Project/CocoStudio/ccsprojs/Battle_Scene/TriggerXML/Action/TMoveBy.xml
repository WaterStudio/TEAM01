<?xml version="1.0" standalone="yes" ?>
<Root>
  <Item Type="NodeTag" Name="NodeTag" CHName="渲染节点标签" Key = "Tag" Default="10000" />
  <Item Type="DoubleUpDown" Name="x" CHName="渲染节点移动X坐标" Key = "x" Default="1.0"/>
  <Item Type="DoubleUpDown" Name="y" CHName="渲染节点移动Y坐标" Key = "y" Default="1.0"/>
  <Item Type="DoubleUpDown" Name="duration" CHName="渲染节点移动持续时间" Key = "Duration" Default="1.0"/>
  <Item Type="ComboBox" Name="IsReverse" CHName="移动是否返回" Key = "IsReverse" Default="1">
    <Childes>
      <Child Name="Yes" CHName="是" ID="1"/>
      <Child Name="No" CHName="否" ID="0"/>
    </Childes>
  </Item>
  <Item Type="ComboBox" Name="IsRepeatForever" CHName="是否永远反复" Key = "IsRepeatForever" Default="1">
    <Childes>
	  <Child Name="Yes" CHName="是" ID="1"/>
	  <Child Name="No" CHName="否" ID="0"/>
    </Childes>
  </Item>
</Root>
